  "description": "Volto add-on to pull data from the farmOS API",
  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "Brian Davis",
  "homepage": "https://github.com/fosten/volto-farmos#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:fosten/volto-farmos.git"
  },
  "publishConfig": {
